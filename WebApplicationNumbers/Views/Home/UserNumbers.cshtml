@model WebApplicationNumbers.Models.NMockList
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>UserNumbers</title>
</head>
<body>
    <div id="idUserNumbers">
        <p>Current user <strong>@ViewBag.SessionID</strong> numbers:</p>
        @{
            for (int i = 0; i < Model.Count; i++)
            {
                <p>@Model[i].Key : @Model[i].Value</p>
            }
        }

        @DateTime.Now

    </div>
</body>
</html>
